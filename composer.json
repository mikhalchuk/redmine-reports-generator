{
    "name": "mikhalchuk/redmine-reports-generator",
    "description": "Generates reports from Redmine",
    "authors": [
        {
            "name": "Sergii Mykhalchuk",
            "email": "sergii.mykhalchuk@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "kbsali/redmine-api": "1.5.0",
        "phpoffice/phpexcel": "1.8.0",
        "pimple/pimple": "3.0.0",
        "symfony/console": "2.6.6",
        "cilex/console-service-provider": "1.1.0",
        "guzzlehttp/guzzle": "5.2.0"
    },
    "require-dev": {
      "squizlabs/php_codesniffer": "2.3.1",
      "phpunit/phpunit": "4.6.4"
    },
    "scripts": {
      "post-install-cmd": [
        "bash bin/setup.sh"
      ]
    },
    "autoload": {
        "psr-4": {
            "RedmineReportsGenerator\\": "src/",
            "RedmineReportsGenerator\\Tests\\": "tests/"
        }
    }
}